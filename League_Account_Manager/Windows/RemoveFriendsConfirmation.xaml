<Window x:Class="League_Account_Manager.Windows.RemoveFriendsConfirmation" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml" Title="BulkAdd" MaxHeight="200" MaxWidth="400" Height="200"
        Width="295" WindowStyle="None" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid Background="#FF252525">

        <ui:Button Content="Remove accounts" Margin="0,77,0,0" VerticalAlignment="Top" Width="200"
                   HorizontalAlignment="Center" Click="Button_Click" />
        <ui:Button Content="Cancel" Margin="0,122,0,0" VerticalAlignment="Top" Width="200"
                   HorizontalAlignment="Center" Click="Button_Click1" />
        <ui:TextBox HorizontalAlignment="Center" Margin="0,10,0,0" TextWrapping="Wrap"
                    Text="Are you sure you want to remove ALL friends?" VerticalAlignment="Top" Width="233" Height="51"
                    IsReadOnly="True" IsUndoEnabled="False" HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Center" Focusable="False" IsHitTestVisible="False" IsEnabled="False" />
    </Grid>
</Window>