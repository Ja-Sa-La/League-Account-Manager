<Page x:Class="League_Account_Manager.views.SettingsEditor"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      Title="SettingsEditor">

    <Grid>
        <TabControl x:Name="DynamicTabControl" />
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="10">
            <Button Content="Reset" Click="ResetButton_Click" Margin="5"/>
            <Button Content="Export" Click="ExportButton_Click" Margin="5"/>
            <Button Content="Import" Click="ImportButton_Click" Margin="5"/>
            <Button Content="Apply to Client" Click="ApplyButton_Click" Margin="5"/>
            <Button Content="Apply to Account" Click="ApplyButton2_Click" Margin="5"/>
            <Button Content="Lock" Click="LockButton_Click" Margin="5"/>
            <Button Content="Unlock" Click="UnlockButton_Click" Margin="5"/>
        </StackPanel>
    </Grid>
</Page>